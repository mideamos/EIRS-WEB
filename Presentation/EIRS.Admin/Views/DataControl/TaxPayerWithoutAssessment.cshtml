@using EIRS.BOL
@using EIRS.Common

@{
    ViewBag.Title = "Tax Payers Without Assessment";

    IList<usp_DC_GetIndividualWithoutAssessment_Result> lstIndividualWithoutAssessment = (IList<usp_DC_GetIndividualWithoutAssessment_Result>)ViewBag.IndividualList;
    IList<usp_DC_GetCompanyWithoutAssessment_Result> lstCompanyWithoutAssessment = (IList<usp_DC_GetCompanyWithoutAssessment_Result>)ViewBag.CompanyList;
}

<div id="content">
    <div class="row">
        <div class="col-xs-12 col-sm-7">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa fa-lg fa-fw fa-th-list"></i>
                Tax Payers Without Assessment
            </h1>
        </div>
        <div class="col-xs-12 col-sm-5 text-right">
        </div>
    </div>
    <section id="widget-grid">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget jarviswidget-color-green" data- id="dvTPIWAList">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Individual Without Assessment</h2>
                        <div class="widget-toolbar">
                        </div>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox">
                            <input class="form-control" type="text">
                        </div>
                        <div class="widget-body no-padding">
                            <table id="tbIndividual" class="table table-striped table-bordered" width="100%">
                                <thead>
                                    <tr>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter User RIN" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Gender" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Title" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter First Name" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Last Name" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Middle Name" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Date of Birth" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter TIN" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Mobile Number 1" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Mobile Number 2" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Email Address 1" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Email Address 2" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Biometric Details" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Tax Office" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Marital Status" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Nationality" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Tax Payer Type" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Economic Activity" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Status" />
                                        </th>
                                    </tr>
                                    <tr>
                                        <th data-class="expand">User RIN</th>
                                        <th>Gender</th>
                                        <th>Title</th>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Middle Number</th>
                                        <th>Date of Birth</th>
                                        <th>TIN</th>
                                        <th>Mobile Number 1</th>
                                        <th>Mobile Number 2</th>
                                        <th>Email Address 1</th>
                                        <th>Email address 2</th>
                                        <th>Biometric Details</th>
                                        <th>Tax Office</th>
                                        <th>Marital Status</th>
                                        <th>Nationality</th>
                                        <th>Tax Payer Type</th>
                                        <th>Economic Activity</th>
                                        <th>Tax Payer Status</th>
                                    </tr>
                                </thead>
                                <tbody id="tbody">
                                    @if (lstIndividualWithoutAssessment != null && lstIndividualWithoutAssessment.Count > 0)
                                    {
                                        foreach (var item in lstIndividualWithoutAssessment)
                                        {
                                            <tr>
                                                <td>@item.IndividualRIN</td>
                                                <td>@item.GenderName</td>
                                                <td>@item.TitleName</td>
                                                <td>@item.FirstName</td>
                                                <td>@item.LastName</td>
                                                <td>@item.MiddleName</td>
                                                <td>@CommUtil.GetFormatedDate(item.DOB)</td>
                                                <td>@item.TIN</td>
                                                <td>@item.MobileNumber1</td>
                                                <td>@item.MobileNumber2</td>
                                                <td>@item.EmailAddress1</td>
                                                <td>@item.EmailAddress2</td>
                                                <td>@item.BiometricDetails</td>
                                                <td>@item.TaxOfficeName</td>
                                                <td>@item.MaritalStatusName</td>
                                                <td>@item.NationalityName</td>
                                                <td>@item.TaxPayerTypeName</td>
                                                <td>@item.EconomicActivitiesName</td>
                                                <td>@item.ActiveText</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
    <section id="widget-grid">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget jarviswidget-color-green" data- id="dvTPCWAList">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Company Without Assessment</h2>
                        <div class="widget-toolbar">
                        </div>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox">
                            <input class="form-control" type="text">
                        </div>
                        <div class="widget-body no-padding">
                            <table id="tbCompany" class="table table-striped table-bordered" width="100%">
                                <thead>
                                    <tr>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Company RIN" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Company Name" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter TIN" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Mobile Number 1" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Mobile Number 2" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Email Address 1" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Email Address 2" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Tax Office" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Tax Payer Type" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Economic Activity" />
                                        </th>
                                        <th class="hasinput" style="width:10%">
                                            <input type="text" class="form-control" placeholder="Filter Status" />
                                        </th>
                                    </tr>
                                    <tr>
                                        <th data-class="expand">Company RIN</th>
                                        <th>Company Name</th>
                                        <th>TIN</th>
                                        <th>Mobile Number 1</th>
                                        <th>Mobile Number 2</th>
                                        <th>Email Address 1</th>
                                        <th>Email address 2</th>
                                        <th>Tax Office</th>
                                        <th>Tax Payer Type</th>
                                        <th>Economic Activity</th>
                                        <th>Tax Payer Status</th>
                                    </tr>
                                </thead>
                                <tbody id="tbody">
                                    @if (lstCompanyWithoutAssessment != null && lstCompanyWithoutAssessment.Count > 0)
                                    {
                                        foreach (var item in lstCompanyWithoutAssessment)
                                        {
                                            <tr>
                                                <td>@item.CompanyRIN</td>
                                                <td>@item.CompanyName</td>
                                                <td>@item.TIN</td>
                                                <td>@item.MobileNumber1</td>
                                                <td>@item.MobileNumber2</td>
                                                <td>@item.EmailAddress1</td>
                                                <td>@item.EmailAddress2</td>
                                                <td>@item.TaxOfficeName</td>
                                                <td>@item.TaxPayerTypeName</td>
                                                <td>@item.EconomicActivitiesName</td>
                                                <td>@item.ActiveText</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsTaxPayerWithoutAssessment.js"></script>
}

