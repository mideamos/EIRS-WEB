@using EIRS.Common;
@{
    ViewBag.Title = "Manager - Review Status Summary Report - Tax Payer List";
}

<div class="title">
    <h1>
        Manager - Review Status Summary Report - Tax Payer List
    </h1>
    <hr>
</div>

<input type="hidden" id="hdnReviewStatusID" value="@ViewBag.ReviewStatusID" />
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">Tax Payer List</div>
        <div class="text-right">
            <a href="@Url.Action("ReviewStatusSummary","Manager")" class="btn btn-redtheme" id="btnCancel"> Cancel </a>
        </div>
    </div>

    <div class="portlet-body" id="dvData">
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblTaxPayerList">
            <thead class="red-th">
                <tr>
                    <th>Tax Payer RIN</th>
                    <th class="none">Tax Payer Name</th>
                    <th>Tax Payer Type </th>
                    <th>Assessment</th>
                    <th>Payment</th>
                    <th>Outstanding</th>
                    <th class="none">Review Status</th>
                    <th class="action-th all">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsManagerReviewStatusSummaryTaxPayer.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}

