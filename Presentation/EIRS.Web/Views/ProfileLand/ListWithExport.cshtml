@{
    ViewBag.Title = "Land List";
}

<div class="title">
    <h1>Lands</h1>
    <hr />
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">
            List of Lands
        </div>
        <div class="actions">
            <a href="@Url.Action("ExportData","ProfileLand")" target="_blank" class="btn btn-theme">Export To Excel</a>
        </div>
    </div>
    <div class="portlet-body">
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblLand">
            <thead class="red-th">
                <tr>
                    <th>RIN</th>
                    <th>Plot Number</th>
                    <th>Occupier</th>
                    <th class="desktop">Land Function</th>
                    <th class="action-th all">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@{Html.RenderPartial("_LandDetailPopup"); }

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsLandListWithExport.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}



