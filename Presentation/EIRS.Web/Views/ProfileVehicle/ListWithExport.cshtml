@{
    ViewBag.Title = "Vehicle List";
}

<div class="title">
    <h1>Vehicles</h1>
    <hr />
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">
            List of Vehicles
        </div>
        <div class="actions">
            <a href="@Url.Action("ExportData","ProfileVehicle")" target="_blank" class="btn btn-theme">Export To Excel</a>
        </div>
    </div>
    <div class="portlet-body">
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblVehicle">
            <thead class="red-th">
                <tr>
                    <th>RIN</th>
                    <th>Reg Number</th>
                    <th>Sub Type</th>
                    <th class="desktop">Purpose</th>
                    <th class="action-th all">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@{Html.RenderPartial("_VehicleDetailPopup"); }

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsVehicleListWithExport.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}



