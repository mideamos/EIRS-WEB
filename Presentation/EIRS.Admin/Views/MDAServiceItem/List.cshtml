@model IList<EIRS.BOL.usp_GetMDAServiceItemList_Result>

@{
    ViewBag.Title = "MDA Service Item List";
}

<div id="content">
    <div class="row">
        <div class="col-xs-12 col-sm-7">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa fa-lg fa-fw fa-life-saver"></i>
                MDA Service Item
            </h1>
        </div>
        <div class="col-xs-12 col-sm-5 text-right">
            <a href="@Url.Action("Add","MDAServiceItem")" class="btn btn-default">
                Add MDA Service Item
            </a>
        </div>
    </div>
    <section id="widget-grid">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget jarviswidget-color-green" data- id="dvMDAServiceItemList">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>MDA Service Item List </h2>
                        <div class="widget-toolbar">
                            <a href="@Url.Action("Add","MDAServiceItem")" class="btn btn-primary">Add MDA Service Item </a>
                        </div>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox">
                            <input class="form-control" type="text">
                        </div>
                        <div class="widget-body no-padding">
                            <table id="tbMDAServiceItem" class="table table-striped table-bordered" width="100%">
                                <thead>
                                    <tr>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter MDA Service Item Reference No" id="txtMDAServiceItemReferenceNo" />
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Revenue Stream" id="txtRevenueStramName" />
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Revenue Sub Stream" id="txtRevenueSubStramName" />
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Item Category" id="txtItemCategoryName"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Item Sub Category " id="txtItemSubCategoryName"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Revenue Agency " id="txtRevenueAgencyName"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter MDA Service Item Name" id="txtMDAServiceItemName" />
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Computation " id="txtComputationName"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Service Base Amount " id="txtServiceBaseAmount"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Percentage " id="txtPercentage"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Service Amount " id="txtServiceAmount"/>
                                        </th>
                                        <th class="hasinput">
                                            <input type="text" class="form-control" placeholder="Filter Status" id="txtStatus"/>
                                        </th>
                                        <th></th>
                                    </tr>
                                    <tr>
                                        <th data-class="expand">MDA Service Item Reference No</th>
                                        <th>Revenue Stream</th>
                                        <th>Revenue Sub Stream</th>
                                        <th>Item Category</th>
                                        <th>Item Sub Category</th>
                                        <th>Revenue Agency</th>
                                        <th>MDA Service Item Name</th>
                                        <th>Computation</th>
                                        <th>Service Base Amount</th>
                                        <th>Percentage</th>
                                        <th>Service Amount</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsMDAServiceItemList.js"></script>
}

