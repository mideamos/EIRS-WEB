@{
    ViewBag.Title = "Pools Betting - Tax Payers";
}

<div class="title">
    <h1>Pools Betting - Tax Payers</h1>
    <hr />
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">
            List of Tax Payers for Pools Betting
        </div>
        <div class="actions">

        </div>
    </div>
    <div class="portlet-body">
        @Html.Hidden("hdnProfileID", (int)ViewBag.ProfileID)
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblPB_TaxPayer">
            <thead class="red-th">
                <tr>
                    <th>Tax Payer Type</th>
                    <th>Tax Payer Name</th>
                    <th>RIN</th>
                    <th>Number</th>
                    <th>Address</th>
                    <th class="action-th all">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@{Html.RenderPartial("_ProfileDetailPopup"); }

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsPBTaxPayerList.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}
