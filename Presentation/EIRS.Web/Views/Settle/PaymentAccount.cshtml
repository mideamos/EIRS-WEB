@{
    ViewBag.Title = "Payment on Account List";
}

<div class="title">
    <h1>Payment on Account</h1>
    <hr />
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">
            List of Payment on Account
        </div>
        <div class="actions">

        </div>
    </div>
    <div class="portlet-body">
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblPoA">
            <thead class="red-th">
                <tr>
                    <th class="all">Ref</th>
                    <th>Tax Payer</th>
                    <th class="all">Date</th>
                    <th>Amount</th>
                    <th>Method</th>
                    <th class="action-th all">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@{Html.RenderPartial("_PaymentAccountDetailPopup"); }


@section Footer {
    <script type="text/javascript" src="~/Scripts/jsPaymentAccountList.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}


