@model EIRS.Models.BuildingViewModel

@{
    ViewBag.Title = "Building Details";
}

<div id="content">
    <div class="row">
        <div class="col-xs-12 col-sm-7">
            <h1 class="page-title txt-color-blueDark">
                <i class="fa fa-lg fa-fw fa-bank"></i>
                Building
            </h1>
        </div>
        <div class="col-xs-12 col-sm-5 text-right">
        </div>
    </div>
    <section id="widget-grid">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                <div class="jarviswidget jarviswidget-color-green" id="dvBuildingDetails">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-eye"></i> </span>
                        <h2>Building</h2>
                    </header>
                    <div class="content">
                        <div class="jarviswidget-editbox">
                            <input class="form-control" type="text">
                        </div>
                        <div class="widget-body">
                            <div class="form-horizontal">
                                <fieldset>
                                    <legend>Building Details</legend>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building RIN</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingRIN
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Tag Number</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingTAGNumber
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Name</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Number</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingNumber
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Street Name</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.StreetName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Off Street Name</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.OffStreetName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Town</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.TownName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">LGA</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.LGAName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Ward</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.WardName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Asset Type</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.AssetTypeName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Type</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingTypeName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Completion</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingCompletionName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Purpose</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingPurposeName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Ownership</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingOwnershipName
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">No Of Units</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.NoOfUnits
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Size – Length (Sqm)</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingSize_Length
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Building Size – Width (Sqm)</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.BuildingSize_Width
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Latitude</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.Latitude
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Longitude</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.Longitude
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label control-label-static">Status</label>
                                        <div class="col-md-10 form-control-static">
                                            @Model.ActiveText
                                        </div>
                                    </div>
                                </fieldset>

                                <div class="form-actions">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <a class="btn btn-default" href="@Url.Action("List","Building")">
                                                Back
                                            </a>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>
