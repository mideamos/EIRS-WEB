@model IList<EIRS.BOL.usp_GetCentralMenuUserBased_Result>

@if (Model != null && Model.Count > 0)
{
    <li class="dropdown">
        <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@Model.FirstOrDefault().ParentMenuName<span class="caret"></span></a>
        <ul class="dropdown-menu">
            @foreach (var item in Model)
            {
                <li><a href="@Url.Action(item.ActionName,item.ControllerName)">@item.CentralMenuName</a></li>
            }
        </ul>
    </li>
}

