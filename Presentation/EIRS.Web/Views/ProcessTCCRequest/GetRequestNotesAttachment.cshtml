@model IList<MAP_TCCRequest_Notes_Document>

@if (Model.Count > 0)
{
    <div class="row">
        <div class="col-sm-12">
            <p>Attachment : </p>
            @foreach (var item in Model)
            {
                <a href="@GlobalDefaultValues.DocumentLink/@item.DocumentPath" target="_blank" download>@item.DocumentName</a> <br />
            }
        </div>
    </div>
}