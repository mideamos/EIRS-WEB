@using EIRS.Common
@{
    ViewBag.Title = "TCC Details";
}


<div class="title">
    <h1>
        TCC Details
    </h1>
    <hr>
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">TCC Details</div>
        <div class="actions">
            @*<a class="btn btn-redtheme" href="@Url.Action("Add","TCCDetail")">
                Add New
            </a>*@
        </div>
    </div>
    <div class="portlet-body">
        <table class="dt-responsive table table-bordered v-middle" width="100%" id="tblTCCDetail">
            <thead class="red-th">
                <tr>
                    <th>Tax Payer Name</th>
                    <th>Tax Payer RIN</th>
                    <th>TIN</th>
                    <th>Tax Year</th>
                    <th>TCC Tax Paid</th>
                    @*<th class="action-th all">Action</th>*@
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Footer {
    <script type="text/javascript" src="~/Scripts/jsTCCDetailList.js?v=@EIRS.Common.GlobalDefaultValues.VersionNumber"></script>
}


