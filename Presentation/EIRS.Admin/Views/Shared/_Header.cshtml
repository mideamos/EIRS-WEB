    <!-- HEADER -->
<header id="header">
    <div id="logo-group">

        <!-- PLACE YOUR LOGO HERE -->
        <span id="logo">
            <img src="/Content/Images/logo.png" alt="EDO State">
        </span>
        <!-- END LOGO PLACEHOLDER -->
    </div>

    <section id="top-right">
        @if (@Model is HelperResult)
        {
            @Model
        }
        else
        {
            if (SessionManager.SystemUserID > 0)
            {
                <div class="pull-right">
                    @Html.Partial("_TopRight")
                </div>
            }
        }
    </section>
</header>
<!-- END HEADER -->