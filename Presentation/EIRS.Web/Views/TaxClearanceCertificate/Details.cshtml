@model usp_GetTaxClearanceCertificateDetails_Result
@{
    ViewBag.Title = "Tax Clearance Certificate Details";
}


<div class="title">
    <h1>
        Tax Clearance Certificate Details
    </h1>
    <hr>
</div>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption">
            Tax Clearance Certificate
        </div>
        <div class="actions">
            <a href="@Url.Action("List","TaxClearanceCertificate")" class="btn btn-theme btn-outline">
                Cancel
            </a>
        </div>
    </div>
    <div class="row view-form">
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label bold">Tax Payer Type: </label>
                <div class="form-control-static">
                    Individual
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">Tax Payer RIN: </label>
                <div class="form-control-static">
                    @Model.TaxPayerRIN
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">TIN: </label>
                <div class="form-control-static">
                    @Model.TaxPayerTIN
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">Serial Number: </label>
                <div class="form-control-static">
                    @Model.SerialNumber
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label bold">Tax Payer Name: </label>
                <div class="form-control-static">
                    @Model.TaxPayerName
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">Tax Year: </label>
                <div class="form-control-static">
                    @Model.TaxYear
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">Request Ref No: </label>
                <div class="form-control-static">
                    @Model.RequestRefNo
                </div>
            </div>
            <div class="form-group">
                <label class="control-label bold">Source of Income: </label>
                <div class="form-control-static">
                    @Model.IncomeSource
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="form-group">
                <label class="control-label bold">Tax Payer Details: </label>
                <div class="form-control-static">
                    @Model.TaxPayerDetails
                </div>
            </div>
        </div>
    </div>
</div>



