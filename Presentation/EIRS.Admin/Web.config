    <!--add this-->
    <security allowRemoteAccess="0" />
    <!--  DefaultConnection is the name of database connection string -->
    <errorLog type="Elmah.SqlErrorLog, Elmah" connectionStringName="ElmahConnection" />
    <!--add this-->
  </elmah>
